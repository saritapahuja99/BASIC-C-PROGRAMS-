#include<stdio.h>

void main(){
  int n,m,count=1;
  printf("Enter a number");
  scanf("%d",&n);
  m=n/2;
  for(int i=2;i<=m;i++){
    if(n%i==0){
      count++;    
    }
  }
  if(n==1){
    printf("Number is neither prime nor composite");
  }
  if(count>1){
    printf("number is not prime");
  }
  if(count==1){
    printf("number is prime");  
  }


}
